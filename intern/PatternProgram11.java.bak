import java.util.Scanner;
class PatternProgram11 
{
	public static void main(String[] args) 
	{
		Scanner scan=new Scanner(System.in);
		System.out.println("Enter the number");
		int num=scan.nextInt();
		for (int i=1 ;i<=num ; i++)
		{
			for (int j=num ;j>=i ; j--)
			{
				System.out.print(j+" ");
			}
			System.out.println();
		}
		for (int i=num ;i>=num-1 ;i-- )
		{
			for (int j=num ;j>=i ;j-- )
			{
				System.out.print(j);
			}
			System.out.println();
		}
	}
}
