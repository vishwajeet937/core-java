import java.util.Scanner;
class PatternProgram8 
{
	public static void main(String[] args) 
	{
		Scanner scan=new Scanner(System.in);
		System.out.println("Enter the number");
		int num=scan.nextInt();
		for (int i=1 ;i<=num ;i++ )
		{
			for (int j=num ;j>=i ;j++)
			{
				System.out.print(j+" ");
			}
			System.out.println();
		}
	}
}
