import java.util.Scanner;
class PatternProgram14 
{
	public static void main(String[] args) 
	{
		Scanner scan=new Scanner(System.in);
		System.out.println("Enter the number");
		int val=scan.nextInt();
		for (int i=0 ;i<val ;i++ )
		{
			for (int j=0 ;j<i ;j++ )
			{
				System.out.print(j);
			}
			System.out.println();
		}
		

	}
}
