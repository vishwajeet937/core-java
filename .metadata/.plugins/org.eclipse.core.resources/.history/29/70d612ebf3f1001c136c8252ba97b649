package Arrays;
import java.util.Scanner;
public class Check3 
{
   public static void main(String[] args)
   { 
	   Scanner scan=new Scanner(System.in);
	   System.out.println("Enter the Number");
	   int i1=scan.nextInt();
	   int[] i2=new int[i1];
	   for(int i=0;i<=i1;i++)
	   {
		   if(i==0)
		   {
		   System.out.println("odd");
		   }
		   else
		    {
			   System.out.println("even");
		    }
	   }
   }
}
