package Logical;

import java.util.Scanner;

public class Demo1 
{
  public static void main(String[] args)
  {
	  Scanner scan=new Scanner(System.in);
	  System.out.println("Enter the Firstvalue");
	  int a=scan.nextInt();
	  Scanner scan1=new Scanner(System.in);
	  System.out.println("Enter the Firstvalue");
	  int b=scan.nextInt();
	  for(int i=a;i<=b;i++)
	    {
		  if(i%2==0)
		  {
		  System.out.print(i);
		  }
		  else if (i%2==1)
		  {
			  System.out.print(i);
		  }
	    }
	  System.out.println();
  }
}
