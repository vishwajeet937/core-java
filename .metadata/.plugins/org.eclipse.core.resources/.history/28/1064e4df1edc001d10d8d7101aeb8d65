package com.app.staticProgram;

public class Bb1
{
	static String name="sonu";
	String name1="kr";
    static
    {
    	System.out.println("static:"+name);
    }
    {
    	System.out.println("non-static:"+name1);
    }
    Bb1(int i)
    {
    	System.out.println("constructor");
    }
	public static void main(String[] args)
	{
		Bb1 b=new Bb1(1);

	}

}
