package ExceptionalHandeling;

public class Check6 
{
    String fName="Sonu";
    String lName="kumar";
    public void m1()
    {
    	try
    	  {
        	System.out.println(fName);
    	  }
    	catch(Exception e)
    	{
    		System.out.println(lName);
    	}
    	finally
    	{
    		System.out.println(fName+" "+lName);
    	}
    }
    public static void main(String[] args)
    {
    	Check6 c=new Check6();
    	c.m1();
    }
}
