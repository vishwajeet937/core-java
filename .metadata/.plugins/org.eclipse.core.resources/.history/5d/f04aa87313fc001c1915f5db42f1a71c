package Logical;

import java.util.Scanner;

public class Demo1 
{
  public static void main(String[] args)
  {
	  Scanner scan=new Scanner(System.in);
	  System.out.println("Enter the value");
	  int a=scan.nextInt();
	  for(int i=0;i<=a;i++)
	    {
		  System.out.print(i);
	    }
	  System.out.println();
  }
}
