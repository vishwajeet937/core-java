package Logical;

import java.util.Scanner;

public class Demo
{
	public static void main(String[] args)
	{
  Scanner scan=new Scanner(System.in);
  System.out.println("Enter the first value");
  int i=scan.nextInt();
  //Scanner scan1=new Scanner(System.in);
  //int b=scan1.nextInt();
  for (i=0;i<=9;i++)
    {
	  System.out.print(i);
    }
  System.out.println();
  }
}
