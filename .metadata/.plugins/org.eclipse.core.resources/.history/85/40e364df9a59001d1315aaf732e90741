package com.app.multiThread;

public class AddThread extends Thread
{
	@Override
	public void run()
	{
		int sum=0;
		for(int i=0;i<5;i++)
		{
			//sum=sum+i;
			//or
			sum+=i;
			System.out.println("Addition :"+sum);
		}
	}

	public static void main(String[] args)
	{
		AddThread at=new AddThread();
		at.start();

	}

}
