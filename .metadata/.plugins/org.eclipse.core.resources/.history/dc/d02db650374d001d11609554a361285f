package com.app.oop;



public class InstanceOfOperator 
{
     public static void m1(Object obj)
     {
    	 if(obj instanceof String)
    	 {
    	/* currentType*/InstanceOfOperator  io=(InstanceOfOperator)obj;//current object
    	io.m2();
    	 }
    	 else
    	 {
    		 System.out.println("hdbc");
    	 }
     }
     public void m2()
     {
    	 
     }
	public static void main(String[] args) 
	{
		InstanceOfOperator iop=new InstanceOfOperator();
		InstanceOfOperator.m1(iop);

	}

}

//Type means CompileTime
//CurrentObject or CurrentReferenct means runTime

