class Test40 
{
	String fname;
	String lname;
	int mobno;
	//creat a parameter constructor
	Test40(String f,String l,int m)
	{
		fname=f;
		lname=l;
		mobno=m;
	}
	//method to display the value
	void m1()
    {
		System.out.println(fname+" "+lname+'\n'+mobno);
    }
	void m2()
	{
		System.out.println("jhj");
	}
	public static void main(String[] args) 
	{
		//creating a object pass value
		Test40 t1=new Test40("sonu","kr",956);
		Test40 t2=new Test40("monu","kr",123);
		System.out.println("Hello World!");
		//calling method to display
		t1.m1();
		t2.m1();
		t2.m2();
	}
}
