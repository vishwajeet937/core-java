class Condation
{
	public static void main(String[] args) 
	{
	    int x=3+3/2%21;
		int x=16%5+3/2;
		//int x1=4+3;
		if(x==4)
		{
			System.out.println("even number");
		}
		else if(x==5)
		{
			System.out.println("odd number");
		}
		else if(x==7)
		{
			System.out.println("true");
		}
		else
		{
			System.out.println("false");
		}
     }
}
