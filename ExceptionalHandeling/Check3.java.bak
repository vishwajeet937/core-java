class Check3 
{
	int a=10;
	int b=0;
	public void m1()throws Exception//Propagation
	{
			try//handling
			{
			    throw new Exception();
				System.out.println("m1"); 
			}
			catch (Exception e)
			{
				System.out.println("catch block");
			}
