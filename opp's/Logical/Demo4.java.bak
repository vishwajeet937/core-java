class Demo4 
{
	public static void main(String[] args) 
	{
		int num=17;
		int a=0;
		for (int i=1;i<=num;i++)
		   {
              if(num%2==0)
			      {
					a++;
			      }
		   }
		   if(a==2)
		{
			System.out.println("prime number");
		}
		else
		  {
			System.out.println("not prime");
		  }
	}
}
