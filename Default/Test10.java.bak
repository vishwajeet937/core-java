class Test10 
{
	public static void main(String[] args) 
	{
      add(40,50);
	  mult(40,50);
	  Divid(30,10);
	  Sub(30,20);
		System.out.println("");
	}//main method end
	static void add(int first,int second)
	{
		System.out.println("add of two no is:"+(first+second));
	}
	static void mult(int first,int second)
	{
		System.out.println("mult of two no is:"+(first*second));
	}
	static void Divid(int first,int second)
	{
		System.out.println("Divid of no is:"+(first/second));
	}
	static void Sub(int first,int second)
	{
		System.out.println("Sub of two no is:"+(first-second));
	}
}
